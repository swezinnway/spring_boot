<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GradeServlet</servlet-name>
  	<display-name>GradeServlet</display-name>
  	<description></description>
  	<servlet-class>com.best.student_manager.controller.GradeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SectionServlet</servlet-name>
  	<display-name>SectionServlet</display-name>
  	<description></description>
  	<servlet-class>com.best.student_manager.controller.SectionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentServlet</servlet-name>
  	<display-name>StudentServlet</display-name>
  	<description></description>
  	<servlet-class>com.best.student_manager.controller.StudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GradeServlet</servlet-name>
  	<url-pattern>/GradeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SectionServlet</servlet-name>
  	<url-pattern>/SectionServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentServlet</servlet-name>
  	<url-pattern>/StudentServlet</url-pattern>
  </servlet-mapping>
</web-app>
